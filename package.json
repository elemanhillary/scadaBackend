{
  "name": "scada",
  "version": "1.0.0",
  "description": "backend for scada",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production npm run migrate && babel-node ./index.js",
    "dev": "NODE_ENV=development npm run migrate:undo && NODE_ENV=development npm run migrate && NODE_ENV=development nodemon ./index.js --exec babel-node",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls && nyc --reporter=lcov --reporter=text-lcov npm test",
    "test": "NODE_ENV=test npm run set:test && NODE_ENV=test mocha --require @babel/register ./tests --timeout 10000 --exit",
    "set:test": "NODE_ENV=test npm run migrate:undo && NODE_ENV=test npm run migrate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "nodemon": "^1.19.1",
    "sequelize-cli": "^5.5.0"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@hapi/joi": "^15.1.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.6",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.5",
    "form-data": "^2.5.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.0",
    "nyc": "^14.1.1",
    "pg": "^7.12.0",
    "sequelize": "^5.12.3",
    "uuid": "^3.3.2"
  }
}
